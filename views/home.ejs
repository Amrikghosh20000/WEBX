<body >
  <div class="card" style="width: 22rem; position: absolute; margin-top: 170px; margin-left: 550px; border-radius: 20px;
  background: linear-gradient(97.7deg, rgba(196, 196, 196, 0.13) 7.47%, #E5E5E5 99.44%);
      backdrop-filter: blur(60px);filter: drop-shadow(8px 8px 6px rgba(0, 0, 0, 0.25));" >
    
    <img src="/image/login.png" class="card-img-top" alt="...">
    <div class="card-body">
       
      <button type="button" class="btn btn-success" id="login" style="margin-left: 25px;">LOGIN</button>
     
      <button type="button" class="btn btn-warning" id="register" style="margin-left: 80px;">REGISTER</button>
   
    </div>
  </div>


  
   
    











   <script>
    let n=document.getElementById('login');
    let m=document.getElementById('register');

      n.onclick=function(){
      let url='http://localhost:4000/login';
      window.open(url);
    }
    m.onclick=function(){
      let url='http://localhost:4000/register';
      window.open(url,'_self');
    }

  </script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
